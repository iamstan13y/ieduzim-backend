{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@iedu/core": [
        "libraries/core/src/index.ts"
      ],
      "@iedu-data-accounts": [
        "libraries/data/accounts/src/index.ts"
      ],
      "@iedu-ui-accounts": [
        "libraries/interface/accounts/src/index.ts"
      ],
      "@iedu-ui-layout": [
        "libraries/interface/layout/src/index.ts"
      ],
      "@iedu-ui-forms": [
        "libraries/interface/forms/src/index.ts"
      ],
      "@iedu-data-system-parameters": [
        "libraries/data/system-parameters/src/index.ts"
      ],
      "@iedu-ui-system-parameters": [
        "libraries/interface/system-parameters/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}